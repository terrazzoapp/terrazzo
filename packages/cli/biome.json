{
  "$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
  "extends": ["../../biome.json"],
  "files": {
    "include": ["./bin/", "./src/", "./test/*.test.ts", "*.tsx"]
  },
  "linter": {
    "rules": {
      "suspicious": {
        "noConsole": "off"
      }
    }
  },
  "overrides": [
    {
      "include": ["./bin/cli.js"],
      "linter": {
        "rules": {
          "suspicious": {
            "noConsole": "off"
          }
        }
      }
    }
  ]
}
