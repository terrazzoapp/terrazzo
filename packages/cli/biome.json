{
  "$schema": "https://biomejs.dev/schemas/1.9.1/schema.json",
  "extends": ["../../biome.json"],
  "files": {
    "include": ["./bin/", "./src/", "./test/"],
    "ignore": ["./test/fixtures/**/*.json", "./test/fixtures/**/*.js", "./test/fixtures/**/*.d.ts"]
  },
  "linter": {
    "rules": {
      "suspicious": {
        "noConsoleLog": "off"
      }
    }
  },
  "overrides": [
    {
      "include": ["./bin/cli.js"],
      "linter": {
        "rules": {
          "suspicious": {
            "noConsoleLog": "off"
          }
        }
      }
    }
  ]
}
