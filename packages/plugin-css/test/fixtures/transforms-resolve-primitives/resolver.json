{
  "$schema": "https://www.designtokens.org/schemas/2025.10/resolver.json",
  "version": "2025.10",
  "name": "transforms-resolve-primitives",
  "resolutionOrder": [
    { "$ref": "#/modifiers/mode" },
    { "$ref": "#/sets/primitives" }
  ],
  "sets": {
    "primitives": {
      "sources": [{ "$ref": "primitives.json" }]
    }
  },
  "modifiers": {
    "mode": {
      "default": "light",
      "contexts": {
        "light": [{ "$ref": "modes.json#light" }],
        "dark": [{ "$ref": "modes.json#dark" }]
      }
    }
  }
}
