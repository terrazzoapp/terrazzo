{
  "$schema": "https://www.designtokens.org/schemas/2025.10/resolver.json",
  "version": "2025.10",
  "resolutionOrder": [
    { "$ref": "#/sets/base" }
  ],
  "sets": {
    "base": {
      "sources": [
        {
          "ease": {
            "$description": "Easing curves",
            "circ-in": { "$type": "cubicBezier", "$value": [0.55, 0, 1, 0.45] },
            "circ-out": { "$type": "cubicBezier","$value": [0.33, 1, 0.68, 1] },
            "circ-in-out": { "$type": "cubicBezier", "$value": [0.85, 0, 0.15, 1] },
            "cubic-in": { "$type": "cubicBezier", "$value": [0.32, 0, 0.67, 0] },
            "cubic-in-out": { "$type": "cubicBezier", "$value": [0.65, 0, 0.35, 1] },
            "cubic-out": { "$type": "cubicBezier", "$value": [0.33, 1, 0.68, 1] },
            "linear": { "$type": "cubicBezier", "$value": [0, 0, 1, 1] }
          },
          "timing": {
            "$description": "Standardized beats",
            "none": { "$type": "duration", "$value": { "value": 0, "unit": "ms" } },
            "instant": { "$type": "duration", "$value": { "value": 50, "unit": "ms" } },
            "quick": { "$type": "duration", "$value": { "value": 100, "unit": "ms" } },
            "moderate": { "$type": "duration", "$value": { "value": 250, "unit": "ms" } },
            "deliberate": { "$type": "duration", "$value": { "value": 500, "unit": "ms" } },
            "slow": { "$type": "duration", "$value": { "value": 1, "unit": "s" } }
          },
          "transition": {
            "$description": "Shared transitions",
            "circ-in-instant": { "$type": "transition", "$value": { "duration": "{timing.instant}", "timingFunction": "{ease.circ-in}", "delay": "{timing.none}"  } },
            "circ-in-quick": { "$type": "transition", "$value": { "duration": "{timing.quick}", "timingFunction": "{ease.circ-in}", "delay": "{timing.none}"  } },
            "circ-in-moderate": { "$type": "transition", "$value": { "duration": "{timing.moderate}", "timingFunction": "{ease.circ-in}", "delay": "{timing.none}"  } },
            "circ-in-deliberate": { "$type": "transition", "$value": { "duration": "{timing.deliberate}", "timingFunction": "{ease.circ-in}", "delay": "{timing.none}"  } },
            "circ-in-slow": { "$type": "transition", "$value": { "duration": "{timing.slow}", "timingFunction": "{ease.circ-in}", "delay": "{timing.none}"  } },
            "circ-in-out-instant": { "$type": "transition", "$value": { "duration": "{timing.instant}", "timingFunction": "{ease.circ-in-out}", "delay": "{timing.none}"  } },
            "circ-in-out-quick": { "$type": "transition", "$value": { "duration": "{timing.quick}", "timingFunction": "{ease.circ-in-out}", "delay": "{timing.none}"  } },
            "circ-in-out-moderate": { "$type": "transition", "$value": { "duration": "{timing.moderate}", "timingFunction": "{ease.circ-in-out}", "delay": "{timing.none}"  } },
            "circ-in-out-deliberate": { "$type": "transition", "$value": { "duration": "{timing.deliberate}", "timingFunction": "{ease.circ-in-out}", "delay": "{timing.none}"  } },
            "circ-in-out-slow": { "$type": "transition", "$value": { "duration": "{timing.slow}", "timingFunction": "{ease.circ-in-out}", "delay": "{timing.none}"  } },
            "circ-out-instant": { "$type": "transition", "$value": { "duration": "{timing.instant}", "timingFunction": "{ease.circ-out}", "delay": "{timing.none}"  } },
            "circ-out-quick": { "$type": "transition", "$value": { "duration": "{timing.quick}", "timingFunction": "{ease.circ-out}", "delay": "{timing.none}"  } },
            "circ-out-moderate": { "$type": "transition", "$value": { "duration": "{timing.moderate}", "timingFunction": "{ease.circ-out}", "delay": "{timing.none}"  } },
            "circ-out-deliberate": { "$type": "transition", "$value": { "duration": "{timing.deliberate}", "timingFunction": "{ease.circ-out}", "delay": "{timing.none}"  } },
            "circ-out-slow": { "$type": "transition", "$value": { "duration": "{timing.slow}", "timingFunction": "{ease.circ-out}", "delay": "{timing.none}"  } },
            "cubic-in-instant": { "$type": "transition", "$value": { "duration": "{timing.instant}", "timingFunction": "{ease.cubic-in}", "delay": "{timing.none}"  } },
            "cubic-in-quick": { "$type": "transition", "$value": { "duration": "{timing.quick}", "timingFunction": "{ease.cubic-in}", "delay": "{timing.none}"  } },
            "cubic-in-moderate": { "$type": "transition", "$value": { "duration": "{timing.moderate}", "timingFunction": "{ease.cubic-in}", "delay": "{timing.none}"  } },
            "cubic-in-deliberate": { "$type": "transition", "$value": { "duration": "{timing.deliberate}", "timingFunction": "{ease.cubic-in}", "delay": "{timing.none}"  } },
            "cubic-in-slow": { "$type": "transition", "$value": { "duration": "{timing.slow}", "timingFunction": "{ease.cubic-in}", "delay": "{timing.none}"  } },
            "cubic-in-out-instant": { "$type": "transition", "$value": { "duration": "{timing.instant}", "timingFunction": "{ease.cubic-in-out}", "delay": "{timing.none}"  } },
            "cubic-in-out-quick": { "$type": "transition", "$value": { "duration": "{timing.quick}", "timingFunction": "{ease.cubic-in-out}", "delay": "{timing.none}"  } },
            "cubic-in-out-moderate": { "$type": "transition", "$value": { "duration": "{timing.moderate}", "timingFunction": "{ease.cubic-in-out}", "delay": "{timing.none}"  } },
            "cubic-in-out-deliberate": { "$type": "transition", "$value": { "duration": "{timing.deliberate}", "timingFunction": "{ease.cubic-in-out}", "delay": "{timing.none}" } },
            "cubic-in-out-slow": { "$type": "transition", "$value": { "duration": "{timing.slow}", "timingFunction": "{ease.cubic-in-out}", "delay": "{timing.none}"  } },
            "cubic-out-instant": { "$type": "transition", "$value": { "duration": "{timing.instant}", "timingFunction": "{ease.cubic-out}", "delay": "{timing.none}"  } },
            "cubic-out-quick": { "$type": "transition", "$value": { "duration": "{timing.quick}", "timingFunction": "{ease.cubic-out}", "delay": "{timing.none}"  } },
            "cubic-out-moderate": { "$type": "transition", "$value": { "duration": "{timing.moderate}", "timingFunction": "{ease.cubic-out}", "delay": "{timing.none}"  } },
            "cubic-out-deliberate": { "$type": "transition", "$value": { "duration": "{timing.deliberate}", "timingFunction": "{ease.cubic-out}", "delay": "{timing.none}"  } },
            "cubic-out-slow": { "$type": "transition", "$value": { "duration": "{timing.slow}", "timingFunction": "{ease.cubic-out}", "delay": "{timing.none}"  } },
            "linear-instant": { "$type": "transition", "$value": { "duration": "{timing.instant}", "timingFunction": "{ease.linear}", "delay": "{timing.none}"  } },
            "linear-quick": { "$type": "transition", "$value": { "duration": "{timing.quick}", "timingFunction": "{ease.linear}", "delay": "{timing.none}" } },
            "linear-moderate": { "$type": "transition", "$value": { "duration": "{timing.moderate}", "timingFunction": "{ease.linear}", "delay": "{timing.none}"  } },
            "linear-deliberate": { "$type": "transition", "$value": { "duration": "{timing.deliberate}", "timingFunction": "{ease.linear}", "delay": "{timing.none}"  } },
            "linear-slow": { "$type": "transition", "$value": { "duration": "{timing.slow}", "timingFunction": "{ease.linear}", "delay": "{timing.none}"  } }
          }
        }
      ]
    }
  }

}
