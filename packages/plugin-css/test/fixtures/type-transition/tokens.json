{
  "ease": {
    "$description": "Easing curves",
    "$type": "cubicBezier",
    "circ-in": { "$value": [0.55, 0, 1, 0.45] },
    "circ-out": { "$value": [0.33, 1, 0.68, 1] },
    "circ-in-out": { "$value": [0.85, 0, 0.15, 1] },
    "cubic-in": { "$value": [0.32, 0, 0.67, 0] },
    "cubic-in-out": { "$value": [0.65, 0, 0.35, 1] },
    "cubic-out": { "$value": [0.33, 1, 0.68, 1] },
    "linear": { "$value": [0, 0, 1, 1] }
  },
  "timing": {
    "$description": "Standardized beats",
    "$type": "duration",
    "none": { "$value": { "value": 0, "unit": "ms" } },
    "instant": { "$value": { "value": 50, "unit": "ms" } },
    "quick": { "$value": { "value": 100, "unit": "ms" } },
    "moderate": { "$value": { "value": 250, "unit": "ms" } },
    "deliberate": { "$value": { "value": 500, "unit": "ms" } },
    "slow": { "$value": { "value": 1, "unit": "s" } }
  },
  "transition": {
    "$description": "Shared transitions",
    "$type": "transition",
    "circ-in-instant": { "$value": { "duration": "{timing.instant}", "timingFunction": "{ease.circ-in}", "delay": "{timing.none}"  } },
    "circ-in-quick": { "$value": { "duration": "{timing.quick}", "timingFunction": "{ease.circ-in}", "delay": "{timing.none}"  } },
    "circ-in-moderate": { "$value": { "duration": "{timing.moderate}", "timingFunction": "{ease.circ-in}", "delay": "{timing.none}"  } },
    "circ-in-deliberate": { "$value": { "duration": "{timing.deliberate}", "timingFunction": "{ease.circ-in}", "delay": "{timing.none}"  } },
    "circ-in-slow": { "$value": { "duration": "{timing.slow}", "timingFunction": "{ease.circ-in}", "delay": "{timing.none}"  } },
    "circ-in-out-instant": { "$value": { "duration": "{timing.instant}", "timingFunction": "{ease.circ-in-out}", "delay": "{timing.none}"  } },
    "circ-in-out-quick": { "$value": { "duration": "{timing.quick}", "timingFunction": "{ease.circ-in-out}", "delay": "{timing.none}"  } },
    "circ-in-out-moderate": { "$value": { "duration": "{timing.moderate}", "timingFunction": "{ease.circ-in-out}", "delay": "{timing.none}"  } },
    "circ-in-out-deliberate": { "$value": { "duration": "{timing.deliberate}", "timingFunction": "{ease.circ-in-out}", "delay": "{timing.none}"  } },
    "circ-in-out-slow": { "$value": { "duration": "{timing.slow}", "timingFunction": "{ease.circ-in-out}", "delay": "{timing.none}"  } },
    "circ-out-instant": { "$value": { "duration": "{timing.instant}", "timingFunction": "{ease.circ-out}", "delay": "{timing.none}"  } },
    "circ-out-quick": { "$value": { "duration": "{timing.quick}", "timingFunction": "{ease.circ-out}", "delay": "{timing.none}"  } },
    "circ-out-moderate": { "$value": { "duration": "{timing.moderate}", "timingFunction": "{ease.circ-out}", "delay": "{timing.none}"  } },
    "circ-out-deliberate": { "$value": { "duration": "{timing.deliberate}", "timingFunction": "{ease.circ-out}", "delay": "{timing.none}"  } },
    "circ-out-slow": { "$value": { "duration": "{timing.slow}", "timingFunction": "{ease.circ-out}", "delay": "{timing.none}"  } },
    "cubic-in-instant": { "$value": { "duration": "{timing.instant}", "timingFunction": "{ease.cubic-in}", "delay": "{timing.none}"  } },
    "cubic-in-quick": { "$value": { "duration": "{timing.quick}", "timingFunction": "{ease.cubic-in}", "delay": "{timing.none}"  } },
    "cubic-in-moderate": { "$value": { "duration": "{timing.moderate}", "timingFunction": "{ease.cubic-in}", "delay": "{timing.none}"  } },
    "cubic-in-deliberate": { "$value": { "duration": "{timing.deliberate}", "timingFunction": "{ease.cubic-in}", "delay": "{timing.none}"  } },
    "cubic-in-slow": { "$value": { "duration": "{timing.slow}", "timingFunction": "{ease.cubic-in}", "delay": "{timing.none}"  } },
    "cubic-in-out-instant": { "$value": { "duration": "{timing.instant}", "timingFunction": "{ease.cubic-in-out}", "delay": "{timing.none}"  } },
    "cubic-in-out-quick": { "$value": { "duration": "{timing.quick}", "timingFunction": "{ease.cubic-in-out}", "delay": "{timing.none}"  } },
    "cubic-in-out-moderate": { "$value": { "duration": "{timing.moderate}", "timingFunction": "{ease.cubic-in-out}", "delay": "{timing.none}"  } },
    "cubic-in-out-deliberate": { "$value": { "duration": "{timing.deliberate}", "timingFunction": "{ease.cubic-in-out}", "delay": "{timing.none}" } },
    "cubic-in-out-slow": { "$value": { "duration": "{timing.slow}", "timingFunction": "{ease.cubic-in-out}", "delay": "{timing.none}"  } },
    "cubic-out-instant": { "$value": { "duration": "{timing.instant}", "timingFunction": "{ease.cubic-out}", "delay": "{timing.none}"  } },
    "cubic-out-quick": { "$value": { "duration": "{timing.quick}", "timingFunction": "{ease.cubic-out}", "delay": "{timing.none}"  } },
    "cubic-out-moderate": { "$value": { "duration": "{timing.moderate}", "timingFunction": "{ease.cubic-out}", "delay": "{timing.none}"  } },
    "cubic-out-deliberate": { "$value": { "duration": "{timing.deliberate}", "timingFunction": "{ease.cubic-out}", "delay": "{timing.none}"  } },
    "cubic-out-slow": { "$value": { "duration": "{timing.slow}", "timingFunction": "{ease.cubic-out}", "delay": "{timing.none}"  } },
    "linear-instant": { "$value": { "duration": "{timing.instant}", "timingFunction": "{ease.linear}", "delay": "{timing.none}"  } },
    "linear-quick": { "$value": { "duration": "{timing.quick}", "timingFunction": "{ease.linear}", "delay": "{timing.none}" } },
    "linear-moderate": { "$value": { "duration": "{timing.moderate}", "timingFunction": "{ease.linear}", "delay": "{timing.none}"  } },
    "linear-deliberate": { "$value": { "duration": "{timing.deliberate}", "timingFunction": "{ease.linear}", "delay": "{timing.none}"  } },
    "linear-slow": { "$value": { "duration": "{timing.slow}", "timingFunction": "{ease.linear}", "delay": "{timing.none}"  } }
  }
}
