  "name": "Microsoft Fluent 2",
  "version": "2025.10",
  "sets": {
    "base": {
      "sources": [{ "$ref": "./palette.tokens.json" }, { "$ref": "./effects.tokens.json" }, { "$ref": "./fonts.tokens.json" }, { "$ref": "./spacing.tokens.json" }]
